<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="com.emotte.cloud.app.user.emerp.mapper.reader.ReEmerpUserMapper">
    <select id="getById" parameterType="java.lang.Long" resultType="com.emotte.cloud.app.user.emerp.model.User">
		select
			*
		from T_USER
		where
		id = #{id}
	</select>
</mapper>